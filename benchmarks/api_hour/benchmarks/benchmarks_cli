#!/usr/bin/env python3
import argparse
import os
from pprint import pprint
import logging

from benchmarks import main

LOG = logging.getLogger(__name__)


ARGS = argparse.ArgumentParser(description='Run benchmarks server.')
ARGS.add_argument('--config_dir', default='%s/etc/benchmarks' % os.getcwd(), help='Config directory')


if __name__ == '__main__':
    main(ARGS.parse_args())